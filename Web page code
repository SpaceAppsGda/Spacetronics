<script type="text/javascript">/* <![CDATA[ */
    jimdoGen002.regModule("module_twitter", {"variant":"default","selector":"#cc-m-twitter-7508197071","username":"Jimdo","count":"2","minTweets":0,"maxTweets":100,"translation":{"errorMessage":"El nombre de usuario de Twitter no existe o Twitter est\u00e1 fuera de servicio en este momento.","lessThanAMinute":"hace menos de un minuto","aboutAMinute":"hace m\u00e1s o menos un minuto","minutes":"hace {minuten} minutos","aboutAnHour":"hace m\u00e1s o menos una hora","hours":"hace {stunden} horas","oneDay":"hace un d\u00eda","days":"hace {tage} d\u00edas"}});
